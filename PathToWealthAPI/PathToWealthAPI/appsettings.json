{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "GeneralRules": [
      {
        "Endpoint": "*:/login,*:/register",
        "Period": "1s",
        "Limit": 1
      }
    ],
    "IpRules": [
      {
        "Ip": "::1/128,192.168.0.0/24",
        "Rules": [
          {
            "Endpoint": "*:/login,*:/register",
            "Period": "4s",
            "Limit": 1
          }
        ]
      }
    ]
  }
}
